{
  "name": "fecool",
  "version": "0.1.16",
  "description": "前端开发工具套件",
  "main": "index.js",
  "bin": {
    "fecool": "./bin/cli.js"
  },
  "scripts": {
    "build": "node bin/cli.js",
    "dev": "node bin/cli.js -d -e development",
    "watch": "node bin/cli.js -d -e development -w",
    "server": "node bin/cli.js -s",
    "babel": "babel ./src --out-dir dist/",
    "test": "node --inspect ./test/index.js"
  },
  "keywords": [
    "fe",
    "fecool"
  ],
  "engines": {
    "node": ">=7.6"
  },
  "dependencies": {
    "@babel/cli": "7.8.4",
