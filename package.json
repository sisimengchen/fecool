    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "browser-sync": "2.26.7",
    "chalk": "2.4.2",
    "commander": "2.20.3",
    "concat-with-sourcemaps": "1.1.0",
    "cssnano": "4.1.10",
    "del": "3.0.0",
    "ejs": "2.7.4",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-cached": "1.1.1",
    "gulp-changed": "3.2.0",
